const express = require("express");
const cors = require("cors");
const app = express();
app.use(cors());
app.use(express.json());
app.get("/", (req, res) => {
  res.json({ message: "Backend Working!" });
});
app.get("/health", (req, res) => {
  res.json({ status: "healthy" });
});
app.listen(8080, () => {
  console.log("Server running");
});
